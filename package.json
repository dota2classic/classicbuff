    "react-input-mask": "^3.0.0-alpha.2",
    "react-modal-hook": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.1",
    "swr": "^0.3.1",
    "throttle-debounce": "^2.3.0"
  },
  "devDependencies": {
