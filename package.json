{
  "name": "front-starter-kit",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "test": "next test",
    "storybook": "start-storybook -p 6006 -s ./",
    "build-storybook": "build-storybook -c .storybook -o build",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "docker": "docker build . -t dota2classic/matchmaking:dotabuff && docker push dota2classic/matchmaking:dotabuff",
    "docker:local": "docker build . -t dota2classic/matchmaking:dotabuff",
    "gql": "graphql-codegen --config codegen.yml",
    "i18n": "babel-node tools/run i18n/compile",
    "i18n-open": "babel-node tools/run i18n/open",
    "i18n-compile": "babel-node tools/run i18n/compile",
    "i18n-pull": "babel-node tools/run i18n/pull",
    "i18n-push": "babel-node tools/run i18n/push",
    "i18n-translate": "babel-node tools/run i18n/translate",
    "i18n-sync": "npm run i18n-pull && npm run i18n && npm run i18n-push && npm run i18n-open",
    "i18n-extract": "node tools/i18n/extract-jsx.js",
    "apigen": "openapi-generator  generate --skip-validate-spec -i https://classicdota.ru/api/api-json -g typescript-fetch -t ./.openapi-generator/typescript-fetch-swr -o ./generated --additional-properties useSingleRequestParameter=false && rm -rf src/api/back && cp -r ./generated src/api/back && rm -rf ./generated",
    "apigen:local": "openapi-generator  generate --skip-validate-spec -i http://localhost:6001/api-json -g typescript-fetch -t ./.openapi-generator/typescript-fetch-swr -o ./generated --additional-properties useSingleRequestParameter=false && rm -rf src/api/back && cp -r ./generated src/api/back && rm -rf ./generated"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-private-methods": "^7.18.6",
